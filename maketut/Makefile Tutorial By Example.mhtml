#header .title-wrapper .subtitle { font-size: 30px; }

#header .social-buttons { display: flex; justify-content: center; }

#header .intro-buttons { display: flex; justify-content: space-between; fle=
x-flow: wrap; }

#header .intro-buttons a { display: inline-block; background-color: rgb(42,=
 161, 152); color: white; font-size: 18px; padding: 16px 32px; border: 0px;=
 border-radius: 8px; cursor: pointer; margin: 20px 0px; }

#header .intro-buttons a:hover { background-color: rgb(39, 150, 141); text-=
decoration: none; }

.body-wrapper { max-width: 1280px; position: relative; margin-left: auto; m=
argin-right: auto; padding: 0px 8px; }

.social-buttons img { width: 25px; height: 25px; margin-right: 10px; }

#left { top: 0px; position: sticky; width: 300px; float: left; height: 100v=
